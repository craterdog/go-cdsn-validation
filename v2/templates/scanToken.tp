
// This method adds a #token# token with the current scanner information to
// the token channel. It returns true if a #token# token was found.
func (v *scanner) scan#Token#() bool {
	var s = v.source[v.nextByte:]
	var matches = bytesToStrings(#token#Scanner.FindSubmatch(s))
	if len(matches) > 0 {
		v.nextByte += len(matches[0])
		v.emitToken(Token#Token#)
		return true
	}
	return false
}
