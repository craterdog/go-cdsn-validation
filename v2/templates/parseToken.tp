
// This method attempts to parse the #token# token. It returns the token
// and whether or not the #token# token was successfully parsed.
func (v *parser) parse#Token#() (#Token#, *Token, bool) {
	var #token#_ #Token#
	var token = v.nextToken()
	if token.Type != Token#Token# {
		v.backupOne()
		return #token#_, token, false
	}
	#token#_ = #Token#(token.Value)
	return #token#_, token, true
}
